# Test Environment Configuration
# This file is used for test environments only

# Test Database (isolated from development)
DATABASE_URL=postgresql://test:test@localhost:5432/imf_test_db

# Test Environment
NODE_ENV=test

# Test Session Secret
SESSION_SECRET=test-secret-for-testing-only

# Test Port (different from dev)
PORT=3001

# Python Framework Configuration for Tests
PYTHON_FRAMEWORK_ENABLED=false
MONITORING_INTERVAL=10
LOG_LEVEL=ERROR

# Disable external services in tests
DISABLE_EXTERNAL_APIS=true
DISABLE_WEBHOOKS=true

# Test-specific settings
TEST_TIMEOUT=30000
TEST_PARALLEL=true
TEST_COVERAGE=true