{
  "name": "mcp-guard",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "dev:full": "./scripts/dev-start.sh",
    "dev:stop": "./scripts/dev-stop.sh",
    "setup": "./scripts/setup-full-dev-environment.sh",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage --reporter=verbose --coverage.reporter=text --coverage.reporter=json --coverage.reporter=lcov",
    "coverage:python": "cd python-framework && python3 -m pytest --cov --cov-report=xml --cov-report=term-missing -q",
    "coverage:aggregate": "tsx scripts/coverage/aggregate-coverage.ts",
    "coverage:all": "npm run test:coverage && npm run coverage:python && npm run coverage:aggregate",
    "test:isolated": "vitest run --pool=forks --isolate",
    "test:db": "TEST_DATABASE_URL=postgresql://test:test@localhost:5432/imf_test_db vitest run",
    "test:db-setup": "./scripts/setup-test-db.sh",
    "test:db-clean": "TEST_DATABASE_URL=postgresql://test:test@localhost:5432/imf_test_db psql -c 'DROP DATABASE IF EXISTS imf_test_db; CREATE DATABASE imf_test_db;'",
    "test:python": "cd python-framework && python -m pytest tests/ -v",
    "test:docker": "vitest run container-integration-real-data.test.ts",
    "test:docker-setup": "cd docker/test-mcp-server && docker-compose up -d",
    "test:docker-clean": "cd docker/test-mcp-server && docker-compose down --remove-orphans",
    "docker:setup": "./docker/setup.sh",
    "docker:dev": "docker-compose up -d",
    "docker:dev-build": "docker-compose up -d --build",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down --volumes --remove-orphans",
    "db:migrate:sql": "bash scripts/apply-migrations.sh",
    "test:ai-storage": "vitest run server/test/enhanced-cross-tenant-sharing.test.ts server/test/long-term/ai-enhanced-continuous-monitoring-tenant-aware.test.ts",
    "test:all-with-docker": "./scripts/run-all-tests-with-docker.sh",
    "python:start": "cd python-framework && python api_integration.py",
    "python:install": "cd python-framework && pip install -r requirements.txt",
    "redis:start": "redis-server --daemonize yes",
    "redis:stop": "redis-cli shutdown",
    "postgres:start": "brew services start postgresql || sudo systemctl start postgresql",
    "postgres:stop": "brew services stop postgresql || sudo systemctl stop postgresql",
    "profiles:cleanup": "tsx scripts/cleanup-test-profiles.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@neondatabase/serverless": "^0.10.4",
    "@radix-ui/react-accordion": "^1.2.4",
    "@radix-ui/react-alert-dialog": "^1.1.7",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
